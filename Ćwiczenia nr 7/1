<?php
if (isset($_POST['button']))
{
    session_start();
    foreach ($_POST as $values => $item)
    {
        echo $values."=>".$item."<br>";
        if ($values!="button"){
            $_SESSION[$values] = $item;
        }
    }
    header("Location: 11.php");
}
